{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng-openapi-gen --input /shared/openapi-spec.json --output src/app/_api --serviceSuffix ApiService && ng serve --host 0.0.0.0 --poll 1000",
    "build": "npm run ng-openapi-gen --input /shared/openapi-spec.json --output dist/_api --serviceSuffix ApiService && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.17",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/forms": "^12.2.17",
    "@angular/localize": "^12.2.17",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@angular/router": "^12.2.17",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^5.2.3",
    "ng-openapi-gen": "^0.23.0",
    "ngx-toastr": "^14.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.5.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.17",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.20.55",
    "jasmine-core": "~3.8.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  }
}